{
  "name": "mautic/mautic-typo3",
    "type": "typo3-cms-extension",
    "description": "An extension to integrate Mautic into TYPO3",
    "version": "1.0.0",
    "license": ["GPL3"],
    "require": {
        "php": ">=7.0.0",
        "mautic/api-library": "^2.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.12"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Mautic\\MauticTypo3\\": "Classes"
        }
    }
}
